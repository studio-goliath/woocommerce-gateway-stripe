    "grunt-postcss": "~0.9.0",
    "grunt-rtlcss": "~2.0.1",
    "grunt-sass": "~2.0.0",
    "grunt-shell": "~2.1.0",
    "grunt-stylelint": "~0.9.0",
    "grunt-wp-i18n": "~1.0.1",
    "mocha": "^3.0.2",
